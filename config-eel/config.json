{
    "Name": "Default",
	"AppName": "eel",
    "Endpoint": "http://localhost:8088",
    "MaxAttempts": 3,
    "InitialDelay": 125,
	"InitialBackoff": 1000,
	"Pad": 0,
	"BackoffMethod": "Exponential",
    "MaxMessageSize": 51200,
    "MessageQueueTimeout": 1000,
    "MessageQueueDepth": 100,
    "WorkerPoolSize": 1,
    "HttpTimeout": 1000,
    "ResponseHeaderTimeout": 1000,
    "MaxIdleConnsPerHost": 100,
    "DuplicateTimeout": 0,
    "HttpTransactionHeader": "X-B3-TraceId",
    "HttpTenantHeader": "Xrs-Tenant-Id",
	"HttpDebugHeader": "X-Debug",
    "CustomProperties": {
        "key" : "value"
    },
    "LogStats": false,
	"TopicPath": "{{/topic}}",
	"CloseIdleConnectionIntervalSec": 0,
	"Version": "1.0",
	"HandlerConfigPath": "",
	"TraceLogParams": {
		"Active": false,
		"FileName": "event.trace.log",
		"LogIncoming": true,
		"LogOutgoing": false,
		"LogParams": {
			"payload": "{{/}}"
		}		
	}
}
